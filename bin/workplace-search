# Hooks will not work correctly due to linked modules from workplace-search unless the react version is linked:
#  https://reactjs.org/warnings/invalid-hook-call-warning.html
cd packages/react-search-ui-views
npm link ../../examples/workplace-search/node_modules/react
cd ../react-search-ui
npm link ../../examples/workplace-search/node_modules/react
cd ../../examples/workplace-search
npm start
